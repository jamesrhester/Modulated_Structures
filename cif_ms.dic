;
         Evaluation
;
         With m as atom_site_Fourier_wave_vector

         temp_Fourier_wave_vector.q_coeff_seq_id =
                [ m.q1_coeff_seq_id, m.q2_coeff_seq_id, m.q3_coeff_seq_id,
